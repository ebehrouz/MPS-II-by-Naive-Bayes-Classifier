*------------------------------------------------------------*;
* Decdata Data Set for Ids;
*------------------------------------------------------------*;
data WORK.Ids_Disease_DD;
  length   Disease                          $ 32
           COUNT                              8
           DATAPRIOR                          8
           TRAINPRIOR                         8
           DECPRIOR                           8
           DECISION1                          8
           DECISION2                          8
           ;

  label    COUNT="Level Counts"
           DATAPRIOR="Data Proportions"
           TRAINPRIOR="Training Proportions"
           DECPRIOR="Decision Priors"
           DECISION1="YE"
           DECISION2="NO"
           ;
  format   COUNT 10.
           ;
Disease="YE";
COUNT=125;
DATAPRIOR=0.00024679316955;
TRAINPRIOR=0.00024679316955;
DECPRIOR=.;
DECISION1=1;
DECISION2=0;
output;
Disease="NO";
COUNT=506372;
DATAPRIOR=0.99975320683044;
TRAINPRIOR=0.99975320683044;
DECPRIOR=.;
DECISION1=0;
DECISION2=1;
output;
;
run;
